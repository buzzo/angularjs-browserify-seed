{
  "name": "myapp",
  "version": "0.0.0",
  "scripts": {
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js  --single-run",
    "build": "browserify -t browserify-css -t brfs app/app.js -o public/bundle.js",
    "serve-watch": "watchify -t browserify-css -t brfs app/app.js -o public/bundle.js",
    "serve-launch": "cd public && w3",
    "serve": "npm run serve-watch & npm run serve-launch"
  },
  "dependencies": {
    "angular": "^1.4.0",
    "angular-route": "^1.4.0",
    "angular-css": "1.0.7",
    "watchify": "^3.2.2"
  },
  "devDependencies": {
    "brfs": "latest",
    "browserify-css": "latest",
    "jasmine-core": "^2.3.4",
    "karma": "~0.12",
    "karma-jasmine": "^0.3.5",
    "karma-chrome-launcher": "^0.1.12",
    "karma-firefox-launcher": "^0.1.6",
    "karma-browserify": "^4.2.1"
  }
}
