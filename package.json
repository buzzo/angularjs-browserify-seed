{
  "name": "myapp",
  "version": "0.0.0",
  "scripts": {
    "prod-jshint": "jshint ./",
    "prod-build": "browserify -t browserify-css -t brfs -d -p [minifyify --no-map] app/app.js -o public/js/main.js",
    "prod": "npm run prod-build & npm run prod-jshint",
    "test": "karma start test/unit/karma.conf.js",
    "test-single-run": "karma start test/unit/karma-coverage.conf.js --single-run",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor test/e2e/protractor.conf.js",
    "serve-watch": "watchify -t browserify-css -t brfs app/app.js -o public/js/main.js",
    "serve-launch": "cd public && w3",
    "serve": "npm run serve-watch & npm run serve-launch"
  },
  "dependencies": {
    "angular": "^1.4.0",
    "angular-route": "^1.4.0",
    "angular-css": "1.0.7"
  },
  "devDependencies": {
    "watchify": "^3.2.2",
    "brfs": "latest",
    "browserify-css": "latest",
    "minifyify": "^7.0.1",
    "jshint": "~2.8.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.12.36",
    "karma-jasmine": "^0.3.5",
    "karma-chrome-launcher": "^0.1.12",
    "karma-firefox-launcher": "^0.1.6",
    "karma-browserify": "^4.2.1",
    "karma-coverage": "^0.4.2",
    "browserify-istanbul": "^0.2.1",
    "protractor": "^2.1.0"
  }
}
